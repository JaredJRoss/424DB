{
  "manifest_version": 2,

  "name": "424_Extension",
  "description": "This extension parses a page for meta-data and adds it to the database",
  "version": "0.3",

  "browser_action": {
    "default_icon": "icon48.png",
    "default_popup": "popup.html",
    "default_title": "Add site to database"
  },
  "icons":
   { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
